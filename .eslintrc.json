{
  "extends": ["standard-react"],
  "plugins": ["react-hooks"],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "mocha": true
  },
  "rules": {
    "func-names": 0,
    "quotes": [2, "backtick"],
    "no-plusplus": 0,
    "no-prototype-builtins": 0,
    "no-restricted-properties": 0,
    "class-methods-use-this": 0,
    "prefer-destructuring": 0,
    "max-len": 0,
    "no-console": 0,
    "react-hooks/rules-of-hooks": "error"
  },
  "globals": {}
}
